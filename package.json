{
	"name": "paw-and-paper",
	"version": "3.4.3",
	"description": "A Discord Bot for roleplay communities, featuring a community-based interactive RPG and powerful tools to roleplay",
	"main": "index.ts",
	"scripts": {
		"start": "git pull origin stable && npm update && rm -r dist && tsc -p tsconfig.json && pm2 start dist/index.js --name paw --time",
		"reload": "git pull origin stable && npm update && rm -r dist && tsc -p tsconfig.json && pm2 reload paw",
		"stop": "pm2 stop paw",
		"logs": "pm2 logs --timestamp --lines 0 paw",
		"test": "ts-node src/index.ts"
	},
	"author": "Maksi",
	"license": "ISC",
	"dependencies": {
		"@octokit/rest": "^19.0.5",
		"@sapphire/async-queue": "^1.5.0",
		"@top-gg/sdk": "^3.1.3",
		"bfd-api-redux": "^1.2.2",
		"crystalid": "^1.0.1",
		"discord.js": "^14.8.0",
		"eslint-plugin-deprecation": "^1.3.3",
		"express": "^4.18.2",
		"express-rate-limit": "^6.7.0",
		"fs": "^0.0.1-security",
		"fuse.js": "^6.6.2",
		"pg": "^8.9.0",
		"pg-hstore": "^2.3.4",
		"reflect-metadata": "^0.1.13",
		"sequelize": "^6.28.0",
		"sequelize-typescript": "^2.1.5",
		"topgg-autoposter": "^2.0.1"
	},
	"devDependencies": {
		"@types/express": "^4.17.15",
		"@types/node": "^18.14.0",
		"@types/validator": "^13.7.12",
		"@typescript-eslint/eslint-plugin": "^5.48.1",
		"@typescript-eslint/parser": "^5.48.1",
		"eslint": "^8.31.0",
		"sequelize-cli": "^6.6.0",
		"ts-node": "^10.9.1",
		"typescript": "^4.9.5"
	}
}